package br.com.fintech.bean;

import java.util.Calendar;

public class Receita {

	private int codigo;
	private String cpf;
	private int tipo;
	private String descricao;
    private double valor_receita;
    private Calendar data_receita;
    private String recorrencia;
    private int qtd_repeticao;
    
    
    
	public Receita() {
		super();
	}


	public Receita(int codigo, String cpf, int tipo, String descricao, double valor_receita, Calendar data_receita,
			String recorrencia, int qtd_repeticao) {
		super();
		this.codigo = codigo;
		this.cpf = cpf;
		this.tipo = tipo;
		this.descricao = descricao;
		this.valor_receita = valor_receita;
		this.data_receita = data_receita;
		this.recorrencia = recorrencia;
		this.qtd_repeticao = qtd_repeticao;
	}






	public int getCodigo() {
		return codigo;
	}

	public String getDescricao() {
		return descricao;
	}

	public void setDescricao(String descricao) {
		this.descricao = descricao;
	}

	public double getValor_receita() {
		return valor_receita;
	}

	public void setValor_receita(double valor_receita) {
		this.valor_receita = valor_receita;
	}

	public Calendar getData_receita() {
		return data_receita;
	}

	public void setData_receita(Calendar data_receita) {
		this.data_receita = data_receita;
	}

	public String getRecorrencia() {
		return recorrencia;
	}

	public void setRecorrencia(String recorrencia) {
		this.recorrencia = recorrencia;
	}

	public int getQtd_repeticao() {
		return qtd_repeticao;
	}

	public void setQtd_repeticao(int qtd_repeticao) {
		this.qtd_repeticao = qtd_repeticao;
	}

	public String getCpf() {
		return cpf;
	}

	public void setCpf(String cpf) {
		this.cpf = cpf;
	}

	public int getTipo() {
		return tipo;
	}

	public void setTipo(int tipo) {
		this.tipo = tipo;
	}
	
	
}
